<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @users.each do |user| %>
        <td><%= image_tag user.get_profile_image(100,100) %></td>
        <td><%= user.name %></td>
        <td>
          <%= link_to "Show", user_path(user) %>
        </td>
      <% end %>
    </tr>
  </tbody>
</table>